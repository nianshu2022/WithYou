{
  "name": "@chronoframe/webgl-image",
  "version": "1.0.0",
  "keywords": [
    "webgl",
    "image-viewer",
    "vue3",
    "zoom",
    "pan",
    "hardware-acceleration",
    "high-performance"
  ],
  "author": {
    "name": "Timothy Yin",
    "email": "master@uniiem.com"
  },
  "license": "MIT",
  "description": "A high-performance WebGL image viewer component for Vue.",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs"
    },
    "./components": {
      "types": "./dist/components.d.ts",
      "import": "./dist/components.mjs",
      "require": "./dist/components.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.mjs",
      "require": "./dist/types.cjs"
    },
    "./dist/webgl-image.css": "./dist/webgl-image.css",
    "./style": "./dist/webgl-image.css"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "vite build --watch",
    "lint": "eslint src --ext .ts,.vue",
    "type-check": "vue-tsc --noEmit"
  },
  "peerDependencies": {
    "vue": "^3.0.0"
  },
  "devDependencies": {
    "@nuxt/eslint": "^1.9.0",
    "@types/node": "^22.0.0",
    "@vitejs/plugin-vue": "^6.0.1",
    "typescript": "^5.0.0",
    "vite": "^7.2.7",
    "vite-plugin-dts": "^4.0.0",
    "vue-tsc": "^2.0.0"
  }
}